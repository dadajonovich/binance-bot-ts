{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitOverride": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["/**/*.ts"],
  "exclude": ["node_modules"]
}
